<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

	<class name="in.sp.beans.Student" table="std_register">
		
		<id name="stdid" column="std_id" />
		
		<property name="name" column="std_name" />
		<property name="rollno" column="std_rollno" />
		<property name="city" column="std_city" />
		
	</class>

</hibernate-mapping>